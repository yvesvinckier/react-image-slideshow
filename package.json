    "gatsby-source-contentful": "^2.0.38",
    "gatsby-source-filesystem": "^2.0.24",
    "gatsby-transformer-sharp": "^2.1.17",
    "gsap": "^2.1.2",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "pixi.js": "^4.8.6",
